{
  "name": "h3",
  "version": "2.0.0-beta.0",
  "description": "Minimal H(TTP) framework built for high performance and portability.",
  "repository": "unjs/h3",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --cache . && prettier -c src test examples docs",
    "lint:fix": "eslint --cache . --fix && prettier -c src test examples docs -w",
    "play:bun": "bun ./test/fixture/bun.ts",
    "play:node": "node --import jiti/register test/fixture/node.ts",
    "play:plain": "node --import jiti/register test/fixture/plain.ts",
    "play:web": "node --import jiti/register test/fixture/web.ts",
    "profile": "0x -o -D .profile -P 'autocannon -c 100 -p 10 -d 40 http://localhost:$PORT' TODO",
    "release": "pnpm test && pnpm build && changelogen --publish --publishTag 2x --prerelease && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest --run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "cookie-es": "^1.1.0",
    "iron-webcrypto": "^1.2.1",
    "ohash": "^1.1.3",
    "rou3": "^0.1.0",
    "ufo": "^1.5.3",
    "uncrypto": "^0.1.3"
  },
  "devDependencies": {
    "0x": "^5.7.0",
    "@types/connect": "^3.4.38",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.6.0",
    "autocannon": "^7.15.0",
    "automd": "^0.3.7",
    "changelogen": "^0.5.5",
    "connect": "^3.7.0",
    "esbuild": "^0.23.0",
    "eslint": "^9.6.0",
    "eslint-config-unjs": "^0.3.2",
    "express": "^4.19.2",
    "get-port": "^7.1.0",
    "get-port-please": "^3.1.2",
    "jiti": "^2.0.0-beta.3",
    "listhen": "^1.7.2",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "supertest": "^7.0.0",
    "typescript": "^5.5.3",
    "unbuild": "3.0.0-rc.6",
    "undici": "^6.19.2",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "crossws": "^0.2.4"
  },
  "peerDependenciesMeta": {
    "crossws": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.4.0"
}
